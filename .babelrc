{
    "presets":[
        "react",
        ["env", {
            "targets": {
                "browsers": "last 2 versions"
            },
            "loose": true,
            "modules": false
        }]
    ],
    "plugins": [
        "react-hot-loader/babel"
    ],
    "env": {
        "test": {
            "plugins": [
                "babel-plugin-transform-es2015-modules-commonjs"
            ]
        }
    }
}